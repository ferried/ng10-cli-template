{
  "compilerOptions": {
    "lib": ["es2017", "dom"],
    "outDir": "../../../../dist/yunzai-g2-charts/schematics",
    "target": "es6",
    "types": ["jasmine", "node"],
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "rootDir": ".",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "exclude": [
    "test.ts",
    "*/files/**/*",
    "test-setup/**/*",
    "**/*.spec.ts",
    "ng-update/test-cases/**/*"
  ]
}


